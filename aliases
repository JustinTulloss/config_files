alias g="git"
alias hgdi="hg diff --color=always --git | less -r"
alias sprunge="git diff master | curl -F 'sprunge=<-' http://sprunge.us | pbcopy"
alias rd="cd ~/Dev/rdio/rdio"
alias web="cd ~/Dev/rdio/rdio/web"
alias cdrvws="curl -F 'rvw=<-' http://cdrv.ws"
alias r="post-review --parent master --guess-description --guess-summary -o -d"
alias gvim="mvim"
alias pg_start="pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start"
